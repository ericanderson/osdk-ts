{
  "name": "@osdk/examples.one.dot.one",
  "private": true,
  "version": "0.0.16",
  "description": "",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./build/types/index.d.ts",
      "import": "./build/js/index.mjs",
      "require": "./build/js/index.js"
    },
    "./*": {
      "types": "./build/types/public/*.d.ts",
      "import": "./build/js/public/*.mjs",
      "require": "./build/js/public/*.js"
    }
  },
  "scripts": {
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "codegen": "rm -rf src/generatedNoCheck/* && osdk unstable typescript generate --outDir src/generatedNoCheck --ontologyPath ontology.json",
    "dev:transpile": "tsup --watch",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "transpile": "tsup",
    "transpileWatch": "tsup --watch",
    "typecheck": "tsc-absolute --build"
  },
  "dependencies": {
    "@osdk/api": "workspace:*",
    "@osdk/generator": "workspace:*",
    "@osdk/legacy-client": "workspace:*"
  },
  "devDependencies": {
    "@osdk/cli": "workspace:*",
    "@types/node": "^18.19.2",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "files": [
    "build/types",
    "build/js",
    "CHANGELOG.md",
    "package.json",
    "*.d.ts"
  ],
  "main": "./build/js/index.js",
  "module": "./build/js/index.mjs",
  "types": "./build/types/index.d.ts"
}
