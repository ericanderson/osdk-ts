{
  "name": "@osdk/examples.basic.cli",
  "private": true,
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./src/public/*.ts",
      "import": "./dist/public/*.mjs",
      "require": "./dist/public/*.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf lib dist tsconfig.tsbuildinfo",
    "dev": "tsup --watch",
    "lint": "eslint . && prettier . --check --ignore-path $(find-up .prettierignore)",
    "prettier": "prettier .",
    "typecheck": "tsc-absolute"
  },
  "dependencies": {
    "@osdk/api": "workspace:*",
    "@osdk/examples.basic.sdk": "workspace:*",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "eslint-config-sane": "workspace:*",
    "mytsup": "workspace:*",
    "ts-expect": "^1.3.0",
    "tsconfig": "workspace:*"
  },
  "publishConfig": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./lib/public/*.d.ts",
      "import": "./dist/public/*.mjs",
      "require": "./dist/public/*.js"
    }
  }
}
