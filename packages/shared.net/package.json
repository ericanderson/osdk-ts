{
  "name": "@osdk/shared.net",
  "version": "1.0.2",
  "description": "",
  "access": "public",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/index.d.ts",
        "default": "./build/esm/index.mjs"
      },
      "require": {
        "types": "./build/cjs/index.d.cts",
        "default": "./build/cjs/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./build/esm/public/*.d.ts",
        "default": "./build/esm/public/*.mjs"
      },
      "require": {
        "types": "./build/cjs/public/*.d.cts",
        "default": "./build/cjs/public/*.js"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "dev:transpile": "tsup --watch",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test": "vitest run",
    "test:package-types": "if [ -d src/public ]; then ls src/public | sed -e 's/\\(.*\\).ts/--include-entrypoints=\\1/' | xargs pnpm attw --pack .; else pnpm attw --pack .; fi",
    "test:watch": "vitest",
    "transpile": "tsup",
    "transpileWatch": "tsup --watch",
    "typecheck": "tsc-absolute --build"
  },
  "dependencies": {
    "@osdk/gateway": "workspace:*",
    "fetch-retry": "^5.0.6"
  },
  "devDependencies": {
    "ts-expect": "^1.3.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "files": [
    "build/cjs",
    "build/esm",
    "CHANGELOG.md",
    "package.json",
    "*.d.ts"
  ],
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.mjs",
  "types": "./build/cjs/index.d.cts",
  "type": "module"
}
