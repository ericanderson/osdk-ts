{
  "name": "@osdk/foundry-sdk-generator",
  "version": "0.200.6",
  "description": "",
  "access": "public",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/index.d.mts",
        "default": "./build/esm/index.mjs"
      },
      "require": {
        "types": "./build/cjs/index.d.ts",
        "default": "./build/cjs/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./build/esm/public/*.d.mts",
        "default": "./build/esm/public/*.mjs"
      },
      "require": {
        "types": "./build/cjs/public/*.d.ts",
        "default": "./build/cjs/public/*.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "dev:transpile": "tsup --watch",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test": "vitest run --config ./vitest.config.mts --pool=forks",
    "test:bin": "./bin/foundry-sdk-generator --help",
    "test:package-types": "if [ -d src/public ]; then ls src/public | sed -e 's/\\(.*\\).ts/--include-entrypoints=\\1/' | xargs pnpm attw --pack .; else pnpm attw --pack .; fi",
    "test:watch": "vitest",
    "transpile": "tsup",
    "transpileWatch": "tsup --watch",
    "typecheck": "tsc-absolute --build"
  },
  "dependencies": {
    "@osdk/api": "workspace:*",
    "@osdk/gateway": "workspace:*",
    "@osdk/generator": "workspace:*",
    "@osdk/legacy-client": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "consola": "^3.2.3",
    "find-up": "^7.0.0",
    "rollup": "^3.28.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "ts-morph": "^19.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@osdk/shared.test": "workspace:*",
    "@types/yargs": "^17.0.29",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "bin": {
    "foundry-sdk-generator": "./bin/foundry-sdk-generator"
  },
  "files": [
    "build/cjs",
    "build/esm",
    "CHANGELOG.md",
    "package.json",
    "*.d.ts"
  ],
  "main": "./build/cjs/index.cjs",
  "module": "./build/esm/index.mjs",
  "types": "./build/cjs/index.d.ts",
  "type": "commonjs"
}
