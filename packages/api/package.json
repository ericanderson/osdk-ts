{
  "name": "@osdk/api",
  "version": "0.1.0",
  "description": "",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./src/public/*.ts",
      "import": "./dist/public/*.mjs",
      "require": "./dist/public/*.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf lib dist tsconfig.tsbuildinfo",
    "dev": "tsup --watch",
    "lint": "eslint . && prettier . --check --ignore-path $(find-up .prettierignore)",
    "prettier": "prettier .",
    "typecheck": "tsc-absolute"
  },
  "dependencies": {
    "fetch-retry": "^5.0.6",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "eslint-config-sane": "workspace:*",
    "mytsup": "workspace:*",
    "ts-expect": "^1.3.0",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./lib/public/*.d.ts",
      "import": "./dist/public/*.mjs",
      "require": "./dist/public/*.js"
    }
  },
  "imports": {
    "#net": "./src/client/internal/net/index.ts",
    "#util": "./src/util/index.ts",
    "#ontology": "./src/ontology/index.ts",
    "#client/converters": "./src/client/internal/conversions/index.ts",
    "#client/query": "./src/client/query/index.ts"
  },
  "keywords": []
}
