{
  "name": "@osdk/cli",
  "version": "0.0.20",
  "description": "",
  "access": "public",
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/index.d.ts",
        "default": "./build/esm/index.mjs"
      },
      "require": {
        "types": "./build/cjs/index.d.cts",
        "default": "./build/cjs/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./build/esm/public/*.d.ts",
        "default": "./build/esm/public/*.mjs"
      },
      "require": {
        "types": "./build/cjs/public/*.d.cts",
        "default": "./build/cjs/public/*.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf lib dist types build tsconfig.tsbuildinfo",
    "dev:transpile": "tsup --watch",
    "fix-lint": "eslint . --fix && dprint fmt --config $(find-up dprint.json)",
    "lint": "eslint . && dprint check  --config $(find-up dprint.json)",
    "test:bin": "./bin/osdk.mjs --help",
    "test:package-types": "if [ -d src/public ]; then ls src/public | sed -e 's/\\(.*\\).ts/--include-entrypoints=\\1/' | xargs pnpm attw --pack .; else pnpm attw --pack .; fi",
    "test:watch": "vitest",
    "transpile": "tsup",
    "transpileWatch": "tsup --watch",
    "typecheck": "tsc-absolute --build"
  },
  "dependencies": {
    "@osdk/api": "workspace:*",
    "@osdk/gateway": "workspace:*",
    "@osdk/generator": "workspace:*",
    "@osdk/shared.net": "workspace:*",
    "archiver": "^6.0.1",
    "conjure-lite": "^0.3.3",
    "consola": "^3.2.3",
    "find-up": "^6.3.0",
    "open": "^9.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.0",
    "@types/ngeohash": "^0.6.4",
    "@types/node": "^18.0.0",
    "@types/yargs": "^17.0.29",
    "ts-expect": "^1.3.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "bin": {
    "osdk": "./bin/osdk.mjs"
  },
  "files": [
    "build/cjs",
    "build/esm",
    "CHANGELOG.md",
    "package.json",
    "*.d.ts"
  ],
  "main": "./build/cjs/index.cjs",
  "module": "./build/esm/index.mjs",
  "types": "./build/cjs/index.d.cts",
  "type": "module"
}
